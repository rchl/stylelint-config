{
    "compilerOptions": {
        "noEmit": true,
        "baseUrl": ".",
        "target": "ES2020",
        "module": "ES2020",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": true,
        "allowSyntheticDefaultImports": true,
        "useUnknownInCatchVariables": false,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "declaration": true,
        "lib": [
            "dom",
            "es2020",
        ],
    },
}
